{
    "name": "opis/cache",
    "description": "A library that helps you work with cached content",
    "keywords": ["cache", "cache drivers", "cache manager"],
    "homepage": "https://www.opis.io/cache",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "psr/simple-cache": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Cache\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}
